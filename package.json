{
  "name": "onontdekt",
  "private": true,
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/csrdelft/onontdekt.git"
  },
  "scripts": {
    "start": "npm run remove-sw && ionic serve --no-open",
    "build": "ionic-app-scripts build --prod",
    "postbuild": "npm run generate-sw",
    "generate-sw": "node ./scripts/build/generate-sw.js",
    "remove-sw": "rm -rf ./www/service-worker.js",
    "test": "karma start ./config/unit-test/karma.conf.js",
    "test-ci": "karma start ./config/unit-test/karma.conf.js --single-run",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts'",
    "lint.prettier": "prettier --write \"./{config,src,scripts}/**/*.{ts,js,json,scss}\"",
    "deploy": "npm run ci && npm run deploy-ios && npm run deploy-android",
    "deploy-ios": "ionic cordova build ios --prod && code-push release-cordova csrdelft.ios ios -d \"Production\"",
    "deploy-android": "ionic cordova build android --prod && code-push release-cordova csrdelft.android android -d \"Production\"",
    "ci": "npm run lint && npm run build"
  },
  "config": {
    "ionic_copy": "./config/app-scripts/copy.config.js",
    "ionic_webpack": "./config/app-scripts/webpack.config.js"
  },
  "dependencies": {
    "@angular/common": "5.2.6",
    "@angular/core": "5.2.6",
    "@angular/forms": "5.2.6",
    "@angular/platform-browser": "5.2.6",
    "@angular/platform-browser-dynamic": "5.2.6",
    "@ionic-native/calendar": "4.5.3",
    "@ionic-native/code-push": "4.5.3",
    "@ionic-native/contacts": "4.5.3",
    "@ionic-native/core": "4.5.3",
    "@ionic-native/in-app-browser": "4.5.3",
    "@ionic-native/keyboard": "4.5.3",
    "@ionic-native/photo-viewer": "4.5.3",
    "@ionic-native/safari-view-controller": "4.5.3",
    "@ionic-native/splash-screen": "4.5.3",
    "@ionic/storage": "2.1.3",
    "@ngrx/effects": "5.1.0",
    "@ngrx/store": "5.1.0",
    "@ngrx/store-devtools": "5.1.0",
    "code-push": "2.0.5",
    "com-sarriaroman-photoviewer": "1.1.15",
    "cordova-android": "7.1.0",
    "cordova-custom-config": "5.0.2",
    "cordova-ios": "4.5.4",
    "cordova-plugin-calendar": "5.0.0",
    "cordova-plugin-code-push": "1.11.7",
    "cordova-plugin-contacts": "3.0.1",
    "cordova-plugin-device": "2.0.1",
    "cordova-plugin-file": "6.0.1",
    "cordova-plugin-file-transfer": "1.7.1",
    "cordova-plugin-inappbrowser": "2.0.2",
    "cordova-plugin-safariviewcontroller": "1.5.2",
    "cordova-plugin-splashscreen": "5.0.2",
    "cordova-plugin-statusbar": "2.4.1",
    "cordova-plugin-whitelist": "1.3.3",
    "cordova-plugin-zip": "3.1.0",
    "cordova-sqlite-storage": "2.2.1",
    "date-fns": "1.29.0",
    "ionic-angular": "3.9.2",
    "ionic-plugin-keyboard": "2.2.1",
    "ionicons": "3.0.0",
    "jwt-decode": "2.2.0",
    "rxjs": "5.5.6",
    "workbox-sw": "2.1.2",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/compiler": "5.2.6",
    "@angular/compiler-cli": "5.2.6",
    "@ionic/app-scripts": "3.1.8",
    "@types/jasmine": "2.8.6",
    "@types/jwt-decode": "2.2.1",
    "@types/node": "9.4.6",
    "angular2-template-loader": "0.6.2",
    "codelyzer": "4.1.0",
    "cordova": "8.0.0",
    "html-loader": "0.5.5",
    "ionic": "3.19.1",
    "jasmine": "3.0.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.11",
    "null-loader": "0.1.1",
    "prettier": "1.10.2",
    "ts-loader": "3.5.0",
    "ts-node": "5.0.0",
    "tslint": "5.9.1",
    "typescript": "2.5.3",
    "workbox-build": "2.1.2"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "com-sarriaroman-photoviewer": {},
      "cordova-custom-config": {},
      "cordova-plugin-calendar": {},
      "cordova-plugin-code-push": {},
      "cordova-plugin-console": {},
      "cordova-plugin-contacts": {},
      "cordova-plugin-device": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-safariviewcontroller": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "cordova-sqlite-storage": {},
      "ionic-plugin-keyboard": {}
    }
  }
}
