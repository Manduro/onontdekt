    "@ionic-native/splash-screen": "4.0.1",
    "@ionic/storage": "2.0.1",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.4",
    "@ngrx/store": "2.2.3",
    "angular2-jwt": "0.2.3",
    "com-sarriaroman-photoviewer": "1.1.10",
